<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webwapsolutions.epizzaappPizzaKingOdense" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Pizza King Odense</name>
    <description>
        
		Pizza King Odense - Restaurant App powered by eRestaurant Suite Pro - leveret af Web Wap Solutions
    </description>
    <author email="support@webwapsolutions.com" href="http://webwapsolutions.com">
        Web Wap Solutions Team - 2017
    </author>
    <dependency id="cordova-plugin-geolocation" />
    <dependency id="cordova-plugin-dialogs" />
    <gap:config-file parent="/manifest/application" platform="android">
        <activity android:launchMode="singleInstance" />
    </gap:config-file>
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />

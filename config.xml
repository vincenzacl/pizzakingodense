<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webwapsolutions.epizzaappPizzaKingOdense" version="1.0.32" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pizza King Odense</name>
    <description>
        
		Pizza King Odense - Restaurant App powered by eRestaurant Suite Pro - leveret af Web Wap Solutions
    </description>
    <author email="support@webwapsolutions.com" href="http://webwapsolutions.com">
        Web Wap Solutions Team - 2017
    </author>
    <dependency id="cordova-plugin-geolocation" />
    <dependency id="cordova-plugin-dialogs" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.SplashScreen" />
        <param name="ios-package" value="CDVSplashScreen" />
    </feature>
    <platform name="android">
        <preference name="Fullscreen" value="false" />
    </platform>
    <platform name="ios">
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="Fullscreen" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
		<preference name="StatusBarStyle" value="blacktranslucent" />
    </platform>
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="SplashScreen" value="generic" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="KeepRunning" value="true" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="LoadingDialog" value="Velkommen..." />
    <preference name="LoadingPageDialog" value="Velkommen..." />
    <preference name="LogLevel" value="WARN" />
    <gap:config-file parent="/manifest/application" platform="android">
        <activity android:launchMode="singleInstance" />
    </gap:config-file>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
	<plugin name="cordova-plugin-statusbar" spec="~2.2.3" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/icon.png" />
    
    <!-- 
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/res/screen/android/res-dk-long-port-ldpi/default.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/res/screen/android/res-dk-long-port-mdpi/default.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/res/screen/android/res-dk-long-port-hdpi/default.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/res/screen/android/res-dk-long-port-xhdpi/default.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="www/res/screen/android/res-dk-long-port-xxhdpi/default.png" />
     -->
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/res/splash/drawable-ldpi/splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/res/splash/drawable-mdpi/splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/res/splash/drawable-hdpi/splash.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/res/splash/drawable-xhdpi/splash.png" />
    <splash density="land-hdpi" src="www/res/splash/drawable-hdpi/splash.png" />
    <splash density="land-ldpi" src="www/res/splash/drawable-ldpi/splash.png" />
    <splash density="land-mdpi" src="www/res/splash/drawable-mdpi/splash.png" />
    <splash density="land-xhdpi" src="www/res/splash/drawable-xhdpi/splash.png" />
    <gap:splash gap:platform="ios" height="480" src="www/res/screen/ios/splash-320x480.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/res/screen/ios/splash-640x960.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/res/screen/ios/splash-640x1136.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/res/screen/ios/splash-768x1024.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/res/screen/ios/splash-1024x768.png" width="1024" />
    <icon gap:platform="ios" height="180" src="www/res/screen/ios/Icon-60@3x.png" width="180" />
    <icon gap:platform="ios" height="60" src="www/res/screen/ios/Icon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="www/res/screen/ios/Icon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="76" src="www/res/screen/ios/Icon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="www/res/screen/ios/Icon-76@2x.png" width="152" />
    <icon gap:platform="ios" height="29" src="www/res/screen/ios/Icon-Small.png" width="29" />
    <icon gap:platform="ios" height="58" src="www/res/screen/ios/Icon-Small@2x.png" width="58" />
    <icon gap:platform="ios" height="40" src="www/res/screen/ios/Icon-Small-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="www/res/screen/ios/Icon-40@2x.png" width="80" />
    <icon gap:platform="ios" height="57" src="www/res/screen/ios/Icon.png" width="57" />
    <icon gap:platform="ios" height="114" src="www/res/screen/ios/Icon@2x.png" width="114" />
    <icon gap:platform="ios" height="72" src="www/res/screen/ios/Icon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="www/res/screen/ios/Icon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="50" src="www/res/screen/ios/Icon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="www/res/screen/ios/Icon-Small-50@2x.png" width="100" />
    <icon gap:platform="ios" height="512" src="www/res/screen/ios/iTunesArtwork.png" width="512" />
    <icon gap:platform="ios" height="1024" src="www/res/screen/ios/iTunesArtwork@2x.png" width="1024" />
</widget>